import java.util.Scanner;

class Soma {

   public static void main( String[] args ) {
      int a, b, soma = 0;
      Scanner s = new Scanner( System.in );
      System.out.println( "Digite dois n√∫meros: ");
      a = s.nextInt();
      b = s.nextInt();
      c = a + b;
      System.out.println( "A soma de a e b: " + c );
   }

}
